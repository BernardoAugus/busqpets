n      produto: {},\n      tipoUsuario: 1\n    };\n  },\n  components: {\n    QList: QList,\n    QItem: QItem,\n    QImg: QImg\n  },\n  mounted: function () {\n    console.log(this.$store.state.produto);\n    this.produto = this.$store.state.produto.produtoSelecionado;\n    this.produto.valor = parseFloat(this.produto.valor);\n    this.produto.quantidade = 1;\n    console.log(this.produto, 'produtoi');\n    this.tipoUsuario = this.$store.state.user.user.profile.tipo;\n  },\n  methods: {\n    pegarAInicialDoOPrimeiroEUltimoNome: function (texto) {\n      if (texto) {\n        var texto2 = '';\n        texto2 = texto.toString().trim();\n\n        if (texto2.indexOf(' ') >= 0) {\n          var textos = [];\n          textos = texto2.split(' ');\n          var quantidadeTextos = 0;\n          quantidadeTextos = textos.length;\n          var iniciais = textos[0][0] + textos[quantidadeTextos - 1][0];\n          return iniciais.toUpperCase();\n        } else {\n          return (texto2[0] + texto2[1]).toUpperCase();\n        }\n      }\n    },\n    adicionarAoCarrinho: function (produto) {\n      this.$q.loading.show();\n      var carrinho = this.$store.state.carrinho.carrinho;\n      var fornecedor = carrinho.findIndex(function (fornecedor) {\n        return produto.fornecedor === fornecedor.fornecedor;\n      });\n\n      if (fornecedor > -1) {\n        var pr = carrinho[fornecedor].produtos.findIndex(function (p) {\n          return p._id === produto._id;\n        });\n\n        if (pr > -1) {\n          carrinho[fornecedor].produtos[pr].quantidade += produto.quantidade;\n          carrinho[fornecedor].total = 0;\n          carrinho[fornecedor].produtos.map(function (p) {\n            carrinho[fornecedor].total += p.quantidade * p.valor;\n          });\n          carrinho[fornecedor].total = carrinho[fornecedor].total.toFixed(2);\n        } else {\n          carrinho[fornecedor].produtos.push(produto);\n          carrinho[fornecedor].total = 0;\n          carrinho[fornecedor].produtos.map(function (p) {\n            carrinho[fornecedor].total += p.quantidade * p.valor;\n          });\n          carrinho[fornecedor].total = carrinho[fornecedor].total.toFixed(2);\n        }\n      } else {\n        carrinho.push({\n          fornecedor: produto.fornecedor,\n          status: 'pendente',\n          produtos: [produto],\n          total: produto.quantidade * produto.valor\n        });\n      }\n\n      console.log(carrinho, 'carrinho');\n      this.$store.commit('carrinho/SET_CARRINHO', carrinho);\n      this.$q.loading.hide();\n    },\n    excluirProduto: function () {\n      console.log('excluir prod', this.produto);\n      this.$emit('excluir-produto', this.produto);\n    },\n    editar: function () {\n      this.$emit('editar-produto', this.produto);\n    },\n    aumentarQuantidade: function (produto) {\n      console.log(produto.quantidade);\n      produto.quantidade += 1;\n    }\n  }\n});","map":{"version":3,"sources":["/Users/bernardosalgado/Desktop/busqpets/template/imports/ui/VisualizarProduto.vue"],"names":["QList","QItem","QImg","module","link","v","exportDefault","data","consumidor","produto","tipoUsuario","components","mounted","console","log","$store","state","produtoSelecionado","valor","parseFloat","quantidade","user","profile","tipo","methods","pegarAInicialDoOPrimeiroEUltimoNome","texto","texto2","toString","trim","indexOf","textos","split","quantidadeTextos","length","iniciais","toUpperCase","adicionarAoCarrinho","$q","loading","show","carrinho","fornecedor","findIndex","pr","produtos","p","_id","total","map","toFixed","push","status","commit","hide","excluirProduto","$emit","editar","aumentarQuantidade"],"mappings":"AAAA,IAAIA,KAAJ,EAAUC,KAAV,EAAgBC,IAAhB;AAAqBC,MAAM,CAACC,IAAP,CAAY,QAAZ,EAAqB;AAACJ,EAAAA,KAAK,EAAC,UAASK,CAAT,EAAW;AAACL,IAAAA,KAAK,GAACK,CAAN;AAAQ,GAA3B;AAA4BJ,EAAAA,KAAK,EAAC,UAASI,CAAT,EAAW;AAACJ,IAAAA,KAAK,GAACI,CAAN;AAAQ,GAAtD;AAAuDH,EAAAA,IAAI,EAAC,UAASG,CAAT,EAAW;AAACH,IAAAA,IAAI,GAACG,CAAL;AAA